<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
            </goals>
            <properties>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.executable>java</exec.executable>
                <exec.mainClass>it.cnr.istc.mw.mqtt.Main</exec.mainClass>
                <exec.vmArgs></exec.vmArgs>
                <exec.appArgs>--research</exec.appArgs>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.mainClass>${packageClassName}</exec.mainClass>
                <exec.vmArgs>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address}</exec.vmArgs>
                <exec.appArgs>--research</exec.appArgs>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-test</actionName>
            <displayName>test</displayName>
            <goals>
                <goal>test</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-build skipping test</actionName>
            <displayName>build skipping test</displayName>
            <goals>
                <goal>install</goal>
            </goals>
            <properties>
                <skipTests></skipTests>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.mainClass>${packageClassName}</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.appArgs>--research</exec.appArgs>
            </properties>
        </action>
    </actions>
